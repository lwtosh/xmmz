page{
  background: #f5f5f5;
}
.ul {
  padding:20rpx;
  margin-bottom: 10rpx;
  background: #fff;
}

.li:nth-of-type(odd) {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%
}

.li {
  height: 158rpx;
  padding: 20rpx;
  color: #fff;
  margin-bottom: 10px;
  position: relative;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}

.name1 {
  font-size: 30rpx;
  font-weight: 500;
  line-height: 1.5;
}

.name2 {
  font-size:22rpx;
  margin-bottom: 10px;
}

.no {
  font-size:46rpx;
}

.del {
  position: absolute;
  right: 0.6rem;
  top: 0.8rem;
  padding: 10rpx 20rpx;
  border: 1px solid #fff043;
  color: #fff043;
  border-radius: 5rpx;
  font-size: 28rpx;
  z-index: 999;
}

.item {
  padding:20rpx;
  background: #fff;
  display: flex;
}

.add {
  width: 35rpx;
  height: 35rpx;
}
.add image {
  vertical-align: -7px;
  width: 100%;
  height: 100%;
}

.text {
  margin-left: 15rpx;
}

.text-index1 {
  font-size: 28rpx;
  color: #666;
  line-height: 2;
}
.text-index2 {
  font-size: 22rpx;
  color: #999;
}
