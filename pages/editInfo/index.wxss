page{
  width: 100%;
  height: 100vh;
  background: #fff;
}
.cont{
  padding-top: .6rem;
}
.cell{
  padding: 10rpx 0;
  width: 92%;
  margin: auto;
  display: flex;
  justify-content: space-between;
  line-height: 70rpx;
  color: #404040;
  font-size: 28rpx;
  border-bottom: 1px solid #f5f5f5
}
.arrow{
  color: #999;
  font-size: 35rpx;
}
.van-cell:not(:last-child)::after{
  right: 16px;
}
.userMin{
  width: 85rpx;
  height: 85rpx;
  border-radius: 50%;
  vertical-align: -21rpx;
}
.userMin image{
  width: 100%;
  height: 100%;
}
.btn{
  height: 98rpx;
  line-height: 98rpx;
  width: 100%;
  text-align: center;
  color: #fff;
  background: #000;
  font-size: 30rpx;
  position: fixed;
  bottom: 0;
}
.van-field__control,.van-cell__value{
  text-align: right !important;
}
.wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.block {
  background-color: #fff;
}